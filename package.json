{
  "name": "botpass",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "pnpm --filter @botpass/web dev",
    "build": "pnpm --filter @botpass/web build",
    "start": "pnpm --filter @botpass/web start",
    "lint": "pnpm --filter @botpass/web lint",
    "typecheck": "pnpm --filter @botpass/web typecheck",
    "test": "pnpm -r --if-present test",
    "db:generate": "prisma generate --schema packages/db/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema packages/db/prisma/schema.prisma",
    "db:deploy": "prisma migrate deploy --schema packages/db/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "6.19.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "prisma": "^6.4.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
